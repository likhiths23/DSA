    "def climbStairs(self, n: int) -> int:\n",
    "    def climb(n):\n",
    "        if n==1 or n==0:\n",
    "            return 1\n",
    "\n",
    "        if dp[n]!=-1:\n",
    "            return dp[n]\n",
    "        else:\n",
    "            dp[n] = climb(n-1) + climb(n-2)\n",
    "            return dp[n]\n",
    "    \n",
    "    dp = [-1]*(n+1)\n",
    "    return dp[n]"
   ]
